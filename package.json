{
  "name": "noflo-rapid-sensemaking-server",
  "version": "0.0.1",
  "description": "rapid sensemaking noflo development server",
  "repository": {
    "type": "git",
    "url": "https://github.com/rapid-sensemaking-framework/noflo-rapid-sensemaking-server.git"
  },
  "scripts": {
    "start": "node node_modules/.bin/noflo-nodejs --open false --debug --verbose --port $PORT --secret $TOP_SECRET",
    "startlocal": "node node_modules/.bin/noflo-nodejs --host 127.0.0.1 --open false --debug --verbose --tls-key=localhost.key --tls-cert=localhost.cert",
    "startinsecure": "node node_modules/.bin/noflo-nodejs --host 127.0.0.1 --open false --debug --verbose --tls-key= --tls-cert=",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Connor Turland <connorturland@gmail.com>",
  "license": "AGPL-3.0",
  "dependencies": {
    "noflo": "^1.1.3",
    "noflo-core": "^0.6.1",
    "noflo-filesystem": "^2.0.1",
    "noflo-flow": "^0.7.0",
    "noflo-nodejs": "^0.11.1",
    "noflo-rsf": "0.0.49",
    "noflo-strings": "^0.4.0"
  }
}
